import random

stop = 0
numbers = []
while stop != 12:
    rand_int = (random.randint(0,9))
    numbers.append(rand_int)
    stop += 1
num = (numbers)
mac_add = str(num[0])+str(num[1])+":"+str(num[2])+str(num[3])+":"+str(num[4])+str(num[5])+":"+str(num[6])+str(num[7])+":"+str(num[8])+str(num[9])+":"+str(num[10])+str(num[11])
print (mac_add)
